{
  "name": "astro-vtbot-website",
  "version": "1.3.6",
  "description": "The Website for the Bag of Tricks for Astro's View Transitions",
  "homepage": "https://github.com/martrapp/astro-vtbot#readme",
  "bugs": {
    "url": "https://github.com/martrapp/astro-vtbot-website/issues"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martrapp/astro-vtbot-website.git"
  },
  "type": "module",
  "scripts": {
    "format": "prettier -w src",
    "dev": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "sizes": "(cd sizes && ./sizes)",
    "relocate": "./bin/relocate.sh",
    "prepDev": "npm link ../astro-vtbot",
    "prepProd": "npm up astro-vtbot",
    "prepShow": "ls -ld node_modules/astro-vtbot ; grep version node_modules/astro-vtbot/package.json"
  },
  "devDependencies": {
    "@astrojs/check": "^0.5.9",
    "@astrojs/mdx": "^2.1.0",
    "@changesets/changelog-github": "0.5.0",
    "@changesets/cli": "^2.27.1",
    "@types/dom-view-transitions": "^1.0.4",
    "@types/node": "^20.11.10",
    "astro": "latest",
    "astro-expressive-code": "^0.33.2",
    "astro-vtbot": "^1.5.0",
    "html2canvas": "^1.4.1",
    "prettier": "^3.1.0",
    "prettier-plugin-astro": "^0.13.0",
    "remark-toc": "^9.0.0",
    "typescript": "^5.3.3"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/martrapp"
  }
}
